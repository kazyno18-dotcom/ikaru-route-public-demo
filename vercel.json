{
    "devCommand": "uvicorn backend.app.main:app --host 0.0.0.0 --port 8000",
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/docs",
            "dest": "/api/index.py"
        },
        {
            "src": "/openapi.json",
            "dest": "/api/index.py"
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/index.py"
        }
    ]
}